# Copy this file to .env and fill values for local development
# WARNING: Never commit real secrets. Use Render dashboard to set production env vars.

# Postgres connection string (Render provided example)
DATABASE_URL=postgresql://user:password@host:5432/dbname

# If your DB provider requires explicit SSL handling
DB_SSL=true

# Session secret for express-session
SESSION_SECRET=change_me_to_a_secure_value

# Node environment
NODE_ENV=development
